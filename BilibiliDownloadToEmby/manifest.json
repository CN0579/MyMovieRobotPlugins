{
  "name": "bilibili_download_scraper",
  "title": "bilibili下载刮削",
  "author": "yanyao",
  "description": "bilibili视频下载刮削，同时附带追更功能（本插件仅供学习技术使用，下载后请在24h内删除）",
  "version": "0.1",
  "requirements": [],
  "configField": [
    {
      "fieldName": "follow_uid_list",
      "fieldType": "String",
      "label": "追更up列表",
      "helperText": "填写需要追更的up的uid，多个uid用英文逗号隔开，如：123456,654321",
      "defaultValue": ""
    },
    {
      "fieldName": "if_get_follow_list",
      "fieldType": "Bool",
      "label": "是否获取登录账号的关注列表作为追更up列表（不会覆盖上方填写的内容，只是叠加）",
      "helperText": "",
      "defaultValue": false
    },
    {
      "fieldName": "font_size",
      "fieldType": "String",
      "label": "弹幕字号",
      "helperText": "填写弹幕字号，如：25",
      "defaultValue": "25"
    },
    {
      "fieldName": "alpha",
      "fieldType": "String",
      "label": "弹幕透明度(0-1)",
      "helperText": "填写弹幕透明度，如：0.8",
      "defaultValue": "1"
    },
    {
      "fieldName": "fly_time",
      "fieldType": "String",
      "label": "滚动弹幕持续时间",
      "helperText": "滚动弹幕持续时间",
      "defaultValue": "7"
    },
    {
      "fieldName": "static_time",
      "fieldType": "String",
      "label": "静态弹幕持续时间",
      "helperText": "静态弹幕持续时间",
      "defaultValue": "5"
    },
    {
      "fieldName": "number",
      "fieldType": "String",
      "label": "保留弹幕数量",
      "helperText": "保留弹幕数量，留空无限制",
      "defaultValue": ""
    }
  ],
  "logoUrl": "/plugins/BilibiliDownloadToEmby/logo.jpg",
  "githubUrl": "https://github.com/yanyao2333/MyMovieRobotPlugins/tree/master/BilibiliDownloadToEmby"
}